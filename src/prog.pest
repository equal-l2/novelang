SP = _{SPACE_SEPARATOR*}
StringContent = {(!"\"" ~ ANY)*}
String = _{ "\"" ~ StringContent ~ "\"" }
Ident = {(!(SEPARATOR|";") ~ ANY)*}
Print = { ^"Print" ~ SP ~ String}
FnBegin = { ^"FnBegin" ~ SP ~ Ident}
FnEnd = { ^"FnEnd" }
Call = {^"Call" ~ SP ~ Ident}
StmtTypes = _{ (Print | FnBegin | FnEnd | Call) }
Statement = _{ SP ~ StmtTypes ~ ";" ~ (SPACE_SEPARATOR | NEWLINE)*}
Prog = _{ SOI ~ Statement* ~ EOI }
