let i be 0 asmut;
while i < 10;
    print "i =", i;
    if i == 2;
        print "i is a prime";
    elif i == 3;
        print "i is a prime";
    elif i == 5;
        print "i is a prime";
    elif i == 7;
        print "i is a prime";
    else;
